				is_mandatory = <True>
				cardinality = <|1..1|>
			>
		>
	>
	["BreastCancerPrognosticStage"] = <
		documentation = <"The prognostic stage of a breast cancer, assessed according to the standard established by American Joint Committee on Cancer (AJCC). Only the prognostic stage is used in US and Canada.">
		name = <"BreastCancerPrognosticStage">
		ancestors = <"EvaluationResultRecorded", ...>
		properties = <
			["stageTimingPrefix"] = (P_BMM_SINGLE_PROPERTY) <
				documentation = <"Mandatory element indicating when the staging was done, in terms of treatment landmarks, either clinical (at diagnosis prior to treatment) (c), pathologic (post-surgical) (p), post-neoadjuvant clinical (yc), or post-neoadjuvant pathologic(yp). The stage timing code is prefix on the N stage. For example, for post-neoadjuvant clinical staging, the N categories are preceded by 'yc' (e.g. ycNX)">
